<MudText Typo="Typo.h5" Color="Color.Primary" GutterBottom>Grand Child Component</MudText>
<MudText Typo="Typo.body1" GutterBottom>
    There is a message: @MessageForGrandChild <br />
    Lucky Number: @LuckyNumberForGrandChild
</MudText>

@code {
